    "version": "0.2.0",
    "configurations": [
        {
            "name": "smartuaq-dev",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_development.dart",
            "args": [
                "--flavor",
                "development" //development or staging or production
            ]
        },
        {
            "name": "smartuaq-stg",
            "request": "launch",
            "type": "dart",
            "program": "lib/main_development.dart",
            "args": [
                "--flavor",
                "staging" //development or staging or production
            ]
        },
        {
            "name": "smartuaq-profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main_staging.dart",
            "args": [
                "--flavor",
                "staging"
            ]
        },
        {
            "name": "smartuaq-release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main_production.dart",
            "args": [
                "--flavor",
                "production"
            ]
        }
    ]
}